lib_LTLIBRARIES += libzbd.la

CFILES = \
	lib/zbd.c \
	lib/zbd_utils.c

HFILES = \
	lib/zbd.h

libzbd_la_DEPENDENCIES = exports
libzbd_la_SOURCES = $(CFILES) $(HFILES)
libzbd_la_CFLAGS = $(AM_CFLAGS)
libzbd_la_LDFLAGS = \
        -Wl,--version-script,$(top_srcdir)/exports \
	-release '$(PACKAGE_VERSION)'


